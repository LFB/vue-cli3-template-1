{
  "name": "vuep-template-cli3",
  "version": "0.1.15",
  "description": "A front-end template built on vue-cli3",
  "author": "liwb <liwb@gmail.com>",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "node build/index.js",
    "lint": "vue-cli-service lint",
    "lint:style": "stylelint **/*.{html,vue} --custom-syntax postcss-html",
    "lint:css": "stylelint **/*.{css,less} --syntax less",
    "analyz": "vue-cli-service build --mode analyz",
    "report": "npm_config_generate_report=true npm run build",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "test": "",
    "gen:comp": "node ./scripts/generateComponent",
    "gen:view": "node ./scripts/generateView",
    "deploy": "npm run build && node build/zip.js",
    "release": "sh build/release.sh"
  },
  "scripts-info": {
    "serve": "运行开发服务器",
    "build": "生产环境执行构建",
    "analyz": "生产环境执行构建打包分析",
    "deploy": "生产环境执行构建并压缩zip包"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run lint",
      "git add"
    ],
    "*.{html,vue,css,less}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@liwb/cloud-utils": "latest",
    "axios": "^0.18.0",
    "lib-flexible": "^0.3.2",
    "magicless": "latest",
    "normalize.css": "^7.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "archiver": "^2.1.1",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "compression-webpack-plugin": "^2.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "html-webpack-include-assets-plugin": "^1.0.6",
    "husky": "^1.1.4",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.0.4",
    "postcss-pxtorem": "^4.0.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "runjs": "^4.3.2",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.1.0",
    "svg-sprite-loader": "^4.1.3",
    "svgo": "^1.1.1",
    "vue-template-compiler": "^2.5.17",
    "webpack-bundle-analyzer": "^3.0.3",
    "webstorm-disable-index": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cklwblove/vue-cli3-template.git"
  },
  "keywords": [
    "vue-cli3",
    "vue",
    "axios"
  ]
}
